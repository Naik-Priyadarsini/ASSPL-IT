
@{
    ViewData["Title"] = "client";
}


    <!-- ============================================-->
    <!-- <section> begin ============================-->
<section style="position: relative; background: none;">
    <div class="bg-holder overlay" style="position: absolute; top: 0; left: 0; width: 100%; height: 250px;">
        <img src="~/img/background-2.jpg" style="object-fit: cover;" />
    </div>
    <div class="container" style="position: relative; z-index: 2;">
        <div class="row pt-6" data-inertia='{"weight":1.5}' style=" align-items: center;">
            <div class="col-md-8 text-white" data-zanim-timeline="{}" data-zanim-trigger="scroll">
                <div class="overflow-hidden">
                    <h1 class="text-white fs-4 fs-md-5 mb-0 lh-1" data-zanim-xs='{"delay":0}'>Client</h1>
                    <div class="nav" aria-label="breadcrumb" role="navigation" data-zanim-xs='{"delay":0.1}'>
                        <ol class="breadcrumb fs-1 ps-0 fw-bold" style="color: white;">
                            <li class="breadcrumb-item"><a class="text-white" asp-controller="Home" asp-action="Index">Home</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Client</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<!-- Main Content -->
<main class="main" id="top">
    <!-- About Section -->
    <section style="padding: 50px 0;">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 pe-lg-3">
                    <img class="rounded-3 img-fluid" src="~/img/why-choose-us.jpg" alt="about" style="width: 100%; border-radius: 10px;" />
                </div>
                <div class="col-lg-6 px-lg-5 mt-6 mt-lg-0" data-zanim-timeline="{}" data-zanim-trigger="scroll">
                    <div class="overflow-hidden">
                        <div class="px-4 px-sm-0" data-zanim-xs='{"delay":0}'>
                            <h5 class="fs-0 fs-lg-1"><span class="fas fa-comment-dots fs-1 me-2" style="color: #dc3545;" data-fa-transform="flip-h"></span>We Are Professional</h5>
                            <p class="mt-3">We resource, train, speak, mentor and encourage; marketplace leaders, business owners and career professionals to be effective in the workplace.</p>
                        </div>
                    </div>
                    <div class="overflow-hidden">
                        <div class="px-4 px-sm-0 mt-5" data-zanim-xs='{"delay":0}'>
                            <h5 class="fs-0 fs-lg-1"><span class="fas fa-palette fs-1 me-2" style="color: #dc3545;" data-fa-transform="shrink-1"></span>We Are Creative</h5>
                            <p class="mt-3">With so many factors to consider when deciding how to characterize your company, wouldn’t it be great to have a group of forward-thinking, well-informed individuals on board who know what they’re doing?</p>
                        </div>
                    </div>
                    <div class="overflow-hidden">
                        <div class="px-4 px-sm-0 mt-5" data-zanim-xs='{"delay":0}'>
                            <h5 class="fs-0 fs-lg-1"><span class="fas fa-stopwatch fs-1 me-2" style="color: #dc3545;" data-fa-transform="grow-1"></span>24/7 Great Support</h5>
                            <p class="mt-3">Design clever and compelling marketing strategies, improve product positioning, and drive conversion rates, Elixir is all time available to guide you.</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Partner Logos Section -->
    <div class="bg-200 py-6" style="background-color: #f8f9fa;">
        <div class="container">
            <div class="row align-items-center" data-zanim-timeline="{}" data-zanim-trigger="scroll">
                <div class="col-4 col-md-2 my-3 overflow-hidden">
                    <img class="img-fluid" src="~/img/partner/planitrentalsLogo.jpg" alt="partnerco" data-zanim-xs="{}" style="max-height: 60px; width: auto;" />
                </div>
                <div class="col-4 col-md-2 my-3 overflow-hidden">
                    <img class="img-fluid" src="~/img/partner/roofconnect.png" alt="tvc" data-zanim-xs="{}" style="max-height: 60px; width: auto;" />
                </div>
                <div class="col-4 col-md-2 my-3 overflow-hidden">
                    <img class="img-fluid" src="~/img/partner/printsome.png" alt="arcade" data-zanim-xs="{}" style="max-height: 60px; width: auto;" />
                </div>
                <div class="col-4 col-md-2 my-3 overflow-hidden">
                    <img class="img-fluid" src="~/img/partner/logo3.png" alt="bearbrand" data-zanim-xs="{}" style="max-height: 60px; width: auto;" />
                </div>
                <div class="col-4 col-md-2 my-3 overflow-hidden">
                    <img class="img-fluid" src="~/img/partner/logo1.png" alt="fast brothers" data-zanim-xs="{}" style="max-height: 60px; width: auto;" />
                </div>
                <div class="col-4 col-md-2 my-3 overflow-hidden">
                    <img class="img-fluid" src="~/img/partner/gfaa.png" alt="harculis beards" data-zanim-xs="{}" style="max-height: 60px; width: auto;" />
                </div>
            </div>
        </div>
    </div>

    <!-- Projects Section -->
    <section class="bg-100" style="padding: 50px 0; background-color: #f1f3f5;">
        <div class="container">
            <div class="text-center mb-6">
                <h3 class="fs-2 fs-md-3">Projects</h3>
                <hr class="short" data-zanim-xs='{"from":{"opacity":0,"width":0},"to":{"opacity":1,"width":"4.20873rem"},"duration":0.8}' data-zanim-trigger="scroll" style="border: 2px solid #dc3545; width: 4.20873rem; margin: 10px auto;" />
            </div>
            <div class="row g-4">
                <div class="col-md-6 col-lg-4">
                    <div class="card" style="border-radius: 10px; box-shadow: 0 4px 8px rgba(0,0,0,0.1);">
                        <a href="news/news.html"><img class="card-img-top" src="~/img/planit.jpg" alt="Featured Image" style="border-top-left-radius: 10px; border-top-right-radius: 10px;" /></a>
                        <div class="card-body" data-zanim-timeline="{}" data-zanim-trigger="scroll">
                            <div class="overflow-hidden">
                                <h5 data-zanim-xs='{"delay":0}'>Plan-it Rentals</h5>
                            </div>
                            <div class="overflow-hidden">
                                <p class="mt-3" data-zanim-xs='{"delay":0.2}'>United State's Largest rental business built with React + Salesforce (Ecommerce)</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-4">
                    <div class="card" style="border-radius: 10px; box-shadow: 0 4px 8px rgba(0,0,0,0.1);">
                        <a href="news/news.html"><img class="card-img-top" src="~/img/10.jpg" alt="Featured Image" style="border-top-left-radius: 10px; border-top-right-radius: 10px;" /></a>
                        <div class="card-body" data-zanim-timeline="{}" data-zanim-trigger="scroll">
                            <div class="overflow-hidden">
                                <h5 data-zanim-xs='{"delay":0}'>BusLyfe</h5>
                            </div>
                            <div class="overflow-hidden">
                                <p class="mt-3" data-zanim-xs='{"delay":0.2}'>Online Bus Ticket Booking website developed using MVC .NET by Abacus System</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-lg-4">
                    <div class="card" style="border-radius: 10px; box-shadow: 0 4px 8px rgba(0,0,0,0.1);">
                        <a href="news/news.html"><img class="card-img-top" src="~/img/roof.webp" alt="Featured Image" style="border-top-left-radius: 10px; border-top-right-radius: 10px;" /></a>
                        <div class="card-body" data-zanim-timeline="{}" data-zanim-trigger="scroll">
                            <div class="overflow-hidden">
                                <h5 data-zanim-xs='{"delay":0}'>RoofConnect</h5>
                            </div>
                            <div class="overflow-hidden">
                                <p class="mt-3" data-zanim-xs='{"delay":0.2}'>Largest national commercial roofing services provider Created with Salesforce.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</main><!-- ===============================================-->
