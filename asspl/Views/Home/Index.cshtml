<style>
    .card-img-top {
        width: 100%;
        height: 320px;
        object-fit: cover;
        object-position: center;
    }
        body

    {
        margin: 0;
        font-family: "Segoe UI", sans-serif;
        background-color: #f9f9f9;
    }

    .container {
        max-width: 1200px;
        margin: 0px auto;
        padding: 0 15px;
    }

    h3 {
        text-align: center;
        font-size: 2rem;
        margin-bottom: 20px;
    }

    .short {
        width: 60px;
        border: 2px solid red;
        margin: 10px auto 40px;
    }

    .grid {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        gap: 20px;
    }

    .card {
        background-color: white;
        border-radius: 8px;
        box-shadow: 0 0 15px rgba(0,0,0,0.1);
        padding: 30px;
        width: 350px;
        position: relative;
        transition: all 0.3s ease;
        cursor: pointer;
    }

        .card::before {
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            background: linear-gradient(to right, #fca311, #fcd34d);
            opacity: 0;
            transition: opacity 0.3s ease;
            z-index: 0;
        }

        .card.active {
            background: linear-gradient(to right, #fca311, #fcd34d);
            color: white;
        }
        .card.active::before {
            opacity: 0.9;
        }

        .card * {
            position: relative;
            z-index: 1;
        }

        .card h4 {
            font-size: 1.25rem;
            font-weight: bold;
            margin-bottom: 10px;
        }

        .card p {
            font-size: 0.95rem;
            color: #555;
        }

    /* .icon-box {
        height: 20px;
        width: 20px;
        border: 2px solid red;
        margin-bottom: 20px;
    } */

    .card.active h4,
    .card.active p {
        color: white;
    }

    .spaced-section {
        margin-top: 100px; /* space above */
        margin-bottom: 100px; /* space after */
    }

    .spaced-heading {
        margin-top: 80px; /* extra space above heading */
    }

</style>
